{
  "name": "postcss-custom-properties-optimizer",
  "version": "0.0.0-development",
  "description": "PostCSS plugin to optimize custom properties",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "postcss-custom-properties-optimizer"
  ],
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs",
      "default": "./lib/index.mjs"
    }
  },
  "scripts": {
    "clean": "rimraf ./lib",
    "build": "rollup --config rollup.config.mjs",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest",
    "types:check": "tsc --noEmit",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/notzheng/postcss-custom-properties-optimizer.git"
  },
  "author": "notzheng",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/notzheng/postcss-custom-properties-optimizer/issues"
  },
  "homepage": "https://github.com/notzheng/postcss-custom-properties-optimizer#readme",
  "peerDependencies": {
    "postcss": "^8.4.21"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "eslint-plugin-jest": "^27.2.1",
    "husky": "^8.0.0",
    "jest": "^29.4.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "rollup": "^3.20.2",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "files": [
    "lib/**/*"
  ]
}
